  "bugs": {
    "url": "https://github.com/18F/identity-site/issues"
  },
  "engines": {
    "node": "10.x.x",
    "yarn": "1.x.x"
  },
  "homepage": "https://github.com/18F/identity-site#readme",
  "main": "webpack.config.js",
  "keywords": [],
  "devDependencies": {
    "nodemon": "^1.19.1",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "7.4.5",
    "aria-accordion": "^1.1.0",
    "babel-loader": "^8.0.6",
    "basscss-sass": "^3.0.0",
    "bootstrap": "^3.4.1",
    "hint.css": "^2.6.0",
    "jquery": "^3.4.1",
    "node-sass": "^4.12.0",
    "normalize.css": "^8.0.1",
    "webpack": "^4.36.1"
  }
}
